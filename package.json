{
	"name": "sageai",
	"version": "1.0.0",
	"description": "Sage AI is a plugin for Obsidian",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"prettify": "bun prettier --write .",
		"lint": "bun eslint .",
		"prepare": "husky"
	},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown"
	},
	"keywords": [
		"obsidian",
		"sage-ai",
		"ai",
		"agent",
		"chatbot",
		"sageai"
	],
	"author": "SudhanPlayz",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.18.126",
		"@types/react": "^19.1.0",
		"@types/react-dom": "^19.1.1",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"husky": "^9.1.7",
		"lint-staged": "^15.5.2",
		"obsidian": "latest",
		"prettier": "3.5.3",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"@ai-sdk/google": "^1.2.7",
		"@openrouter/ai-sdk-provider": "^0.4.5",
		"ai": "^4.3.2",
		"date-fns": "^4.1.0",
		"lucide-react": "^0.487.0",
		"ollama-ai-provider": "^1.2.0",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"zod": "^3.24.2"
	}
}
